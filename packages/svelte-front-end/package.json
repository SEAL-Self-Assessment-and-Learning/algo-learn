{
  "name": "svelte-front-end",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "$npm_execpath run check && prettier --check . && eslint ."
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.26.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.0.14",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "clsx": "^2.1.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.3.0",
    "globals": "^16.0.0",
    "lucide-svelte": "^0.482.0",
    "prettier": "^3.5.3",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.23.1",
    "svelte-check": "^4.1.5",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.0.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript-eslint": "^8.32.1",
    "vite-plugin-devtools-json": "^0.1.0"
  },
  "dependencies": {
    "@dnd-kit-svelte/core": "^0.0.8",
    "@dnd-kit-svelte/sortable": "^0.0.8",
    "@icons-pack/svelte-simple-icons": "^5.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "bits-ui": "^1.3.13",
    "katex": "^0.16.21"
  }
}
